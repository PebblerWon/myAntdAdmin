{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import",{"libraryName":"antd","style":"css"}]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://jsonplaceholder.typicode.com/",
      
      "changeOrigin": true,
      "pathRewrite": { "^/api/v1" : "" }
    },
    "/api2":{
        "target":"https://randomuser.me",
        "changeOrigin":true,
        "pathRewrite":{"^/api2/v1":""}
    }
  }
}
